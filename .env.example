# Copy this file to .env and fill in your Algolia credentials
ALGOLIA_APP_ID=YourAlgoliaAppId
ALGOLIA_API_KEY=YourAlgoliaAdminApiKey
ALGOLIA_INDEX_NAME=YourAlgoliaIndexName

# Optional: enable dry run to prepare records without uploading
# ALGOLIA_DRY_RUN=1
# Optional: provide a base site URL to generate absolute links in Algolia records
# If not provided, the indexer will try to read `url` from docusaurus.config.js
ALGOLIA_SITE_URL=https://your-site.example.com
